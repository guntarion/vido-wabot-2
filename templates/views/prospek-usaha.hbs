<script>
	document.getElementById('capture-data').addEventListener('click', function () {
		var websiteUrl = document.getElementById('website').value;
		if (websiteUrl) {
			fetch('/api/fetch-text', {
				method: 'POST',
				headers: {
					'Content-Type': 'application/json'
				},
				body: JSON.stringify({ url: websiteUrl })  // Send the URL as part of the request body
			})
				.then(response => response.json())
				.then(data => {
					if (data.success) {
						document.getElementById('konten-frontpage').value = data.data;  // Display fetched text in the textarea
						console.log('Fetched Text:', data.data);
					} else {
						console.error('Failed to fetch text:', data.message);
						alert('Failed to fetch text: ' + data.message);
					}
				})
				.catch(error => {
					console.error('Error:', error);
					alert('An error occurred while fetching text');
				});
		} else {
			alert('Please enter a valid URL.');
		}
	});

	document.getElementById('fetch-seo').addEventListener('click', function () {
    var websiteUrl = document.getElementById('website').value;
    if (websiteUrl) {
        fetch('/api/fetch-seo', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ url: websiteUrl })
        })
        .then(response => response.json())
        .then(data => {
            if (data.success) {
                console.log('SEO Data:', data.data);
                // Update the textarea with SEO information
                var infoTextArea = document.getElementById('info-usaha');
                var seoText = `Title: ${data.data.title}\n` +
                              `Meta Description: ${data.data.metaDescription}\n` +
                              `First H1: ${data.data.firstH1}`;
                infoTextArea.value = seoText;
                // alert('SEO data fetched successfully! Check the textarea for details.');
            } else {
                console.error('Failed to fetch SEO data:', data.message);
                alert('Failed to fetch SEO data: ' + data.message);
            }
        })
        .catch(error => {
            console.error('Error:', error);
            alert('An error occurred while fetching SEO data');
        });
    } else {
        alert('Please enter a valid URL.');
    }
});

document.getElementById('buat-rekomendasi').addEventListener('click', function () {
	var seometadata = document.getElementById('info-usaha').value;
	var kontenfrontpage = document.getElementById('konten-frontpage').value;

	// Show the spinner
	document.querySelector('.css3-spinner').style.display = 'block';

	fetch('/api/openai-rekomendasi-seragam', {
		method: 'POST',
		headers: {
			'Content-Type': 'application/json',
		},
		body: JSON.stringify({
			seometadata: seometadata,
			kontenfrontpage: kontenfrontpage,
		}),
	})
	.then(response => response.json())
	.then(data => {
		// Hide the spinner
		document.querySelector('.css3-spinner').style.display = 'none';
		// handle the response data
		document.getElementById('rekomendasi-produk').value = data.message;
	})
	.catch((error) => {
		// Hide the spinner in case of error
		document.querySelector('.css3-spinner').style.display ='none';
		console.error('Error:', error);
	});
});

	document.getElementById('buat-penawaran').addEventListener('click', function () {
		var seometadata = document.getElementById('info-usaha').value;
		var rekomendasiproduk = document.getElementById('rekomendasi-produk').value;

		// Show the spinner
		document.querySelector('.css3-spinner').style.display = 'block';

		fetch('/api/openai-buat-penawaran', {
			method: 'POST',
			headers: {
				'Content-Type': 'application/json',
			},
			body: JSON.stringify({
				seometadata: seometadata,
				rekomendasiproduk: rekomendasiproduk,
			}),
		})
			.then(response => response.json())
			.then(data => {
				// Hide the spinner
				document.querySelector('.css3-spinner').style.display = 'none';
				// handle the response data
				document.getElementById('narasi-penawaran').value = data.message;
			})
			.catch((error) => {
				// Hide the spinner in case of error
				document.querySelector('.css3-spinner').style.display = 'none';
				console.error('Error:', error);
			});
	});

</script>